{
  "name": "keeper-service",
  "version": "1.0.0",
  "description": "Monitoring and redeeming service for Keeper",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "build": "tsc",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "dev": "ts-node ./src/server.ts",
    "start": "node dist/src/server.js",
    "test": "jest",
    "db-sync": "cross-env DB_SYNC=true ts-node ./src/server.ts",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix"
  },
  "dependencies": {
    "connection-string-parser": "^1.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "8.2.0",
    "nodemon": "2.0.6",
    "pg": "^8.5.1",
    "typeorm": "^0.2.29",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.19",
    "@types/node": "14.14.14",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "4.1.3"
  }
}
